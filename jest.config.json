{
    "transform": {
        "^.+\\.tsx?$": [
            "ts-jest"
        ],
        "\\.svg$": "jest-transformer-svg"
    },
    "moduleNameMapper": {
        "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/src/jest.setup.ts"]
}